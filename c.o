# App Settings secret key, created in mission area account to provide info to developers.
output "cognito_settings_secret" {
  value = module.appclient-settings.secret_ids
}

output "cognito_settings_secret_name" {
  value = local.appclient_settings_secret_name
}

output "user_pool_id" {
  value = data.terraform_remote_state.userpool.outputs.id
}

output "user_pool_client_id" {
  value = resource.aws_cognito_user_pool_client.this.id
}

